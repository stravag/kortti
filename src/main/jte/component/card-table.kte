@import ch.ranil.kortti.domain.card.Card

@param cards: List<Card>

<table>
    <thead>
    <tr>
        <td>Type</td>
        <td>Content</td>
        <td>Created at</td>
        <td></td>
    </tr>
    </thead>
    <tbody id="cards-table">
    @for(card in cards)
        @template.component.card-row(card = card)
    @endfor
    </tbody>
</table>
